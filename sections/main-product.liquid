{% comment %}theme-check-disable TemplateLength{% endcomment %}
{{ 'section-main-product.css' | asset_url | stylesheet_tag }}
{{ 'component-accordion.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-rte.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.css' | asset_url | stylesheet_tag }}
{{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
{{ 'photoswipe.min.css' | asset_url | stylesheet_tag }}
{{ 'default-skin.min.css' | asset_url | stylesheet_tag }}
<link rel="stylesheet" href="{{ 'component-deferred-media.css' | asset_url }}" media="print" onload="this.media='all'">
<script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

{% assign readmore_split = product.description | split: '<!-- more -->' %}

{%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
{% comment %}

                {% if product.media.size > 8 and c == 8  %}
                
                <span class="count-more c-desk" data-index="{{c }}">{{product.media.size | minus : foloop.index }}+</span>
              
                      {% endif %}
             {% if product.media.size > 5 and c == 5 %}
              
                <span class="count-more c-mobile" data-index="{{c | minus: 1 }}">{{product.media.size | minus :c }}+</span>
              
                      {% endif %}
                       {%- if media.media_type == 'video' or media.media_type =='external_video' -%}
                    <div class="product-single__thumbnail-badge">
                      {% include 'icon-video-badge-full-color' %}
                    </div>
                    {% endif %}
{% endcomment %}
<section class="page-width">
{% render 'breadcrumb-product' %}
</section>
<section class="page-width main-wrap-product">
<div class="product grid grid--1-col {% if product.media.size > 0 %}grid--2-col-desktop{% else %}product--no-media{% endif %}">
  <div class="grid__item product__media-wrapper-new">
    <div class="d-flex m-0 slider-wrapper">
     <div class="col-xl-2 col-lg-2 col-md-3 col-12 main-nav-wrapper {% if product.media.size > 1 %} product-media-show {% else %} product-media-hide {% endif %} ">
        <div class="pro-nav m-auto">
        {% assign c = 0 %}
          <ul class="pro-nav-ul list-unstyled mb-0">
          {% assign rpt = false %}
             {% assign rpt1 = false %}
            {% for media in product.media %}   
                    

               
             {% case media.media_type %}
              {% when 'image' %}
                <li class="nav-li" data-index="{{c}}">
      {% assign c = c| plus:1 %}
            
                  <a href="javascript:void(0)">


                  {%
                      render 'rimg',
                      class : 'img-fluid m-auto lazyload',
                      img: media.preview_image,
                      size: '70x70',
                      lazy : false
                    %}
                   
                  </a>
                </li>
               
           {% endcase %}
              
            {% endfor %}
            {% if c <=5 %}
              {% assign rpt=true %}
               {% for media in product.media %}   
             {% case media.media_type %}
              {% when 'image' %}
                <li class="nav-li" data-index="{{c}}">
      {% assign c = c| plus:1 %}
            
                  <a href="javascript:void(0)">


                  {%
                      render 'rimg',
                      class : 'img-fluid m-auto lazyload',
                      img: media.preview_image,
                      size: '70x70',
                      lazy : false
                    %}
                   
                  </a>
                </li>
               
           {% endcase %}
              
            {% endfor %}
                {% if rpt1 == ture %}
                        {% for media in product.media %}   
                        {% case media.media_type %}
                        {% when 'image' %}
                        <li class="nav-li" data-index="{{c}}">
                        {% assign c = c| plus:1 %}
                        
                        <a href="javascript:void(0)">
                        
                        
                        {%
                        render 'rimg',
                        class : 'img-fluid m-auto lazyload',
                        img: media.preview_image,
                        size: '70x70',
                        lazy : false
                        %}
                        
                        </a>
                        </li>
                        
                        {% endcase %}
                        
                        {% endfor %}
                {% endif %}
            {% endif %}
          </ul>

        <input type="hidden" value="5" class="slide-count">
     <input type="hidden" value="{{ product.media.size }}" class="image-count">
    
        <div class="pro-count open-gallery">

     
          
          <span class="count-more c-desk">     {% if product.media.size > 6  %}{{product.media.size | minus : 6}} {% else %} {{ product.media.size }} {% endif %}+</span>
          
           

          
          <span class="count-more c-mobile">            {% if product.media.size > 5  %}{{product.media.size | minus :5 }}  {% else %} {{ product.media.size }} {% endif %}+</span>
          
            
            {%
                      render 'rimg',
                      class : 'img-fluid m-auto lazyload  count-image',
                      img: product.featured_image,
                      size: '70x70',
                      lazy : false
                    %}
                   
        </div>
          
        </div>
       
      </div>
      <div class="col-xl-10 col-lg-10 col-md-9 col-12 main-slider-wrapper px-md-1 px-0  ">             
        <div class="product-slider slider-for mb-0 gallery">
           {% assign c1 = 0 %}
          {% for media in product.media %}
           
              {% case media.media_type %}
              {% when 'image' %}
              <div class="text-center img_box" >
                <a href="javascript:void(0);" data-index="{{c1 }}" class="slide-link"  data-video="false" >
                  <div class="img_wrap zoom">
                  {%
                      render 'rimg',
                      class : 'img-fluid m-auto lazyload',
                      img: media.src,
                      size: '700x700',
                      lazy : false
                    %}
                 {% assign c1 = c1| plus:1 %}
                    

                  </div>
                </a>
                </div>       
               {% endcase %}
           
          {% endfor %}
          {% if c1 <= 5 %}
                                  {% for media in product.media %}
                                  
                                  {% case media.media_type %}
                                  {% when 'image' %}
                                  <div class="text-center img_box" >
                                  <a href="javascript:void(0);" data-index="{{c1 }}" class="slide-link"  data-video="false" >
                                  <div class="img_wrap zoom">
                                  {%
                                  render 'rimg',
                                  class : 'img-fluid m-auto lazyload',
                                  img: media.src,
                                  size: '700x700',
                                  lazy : false
                                  %}
                                  {% assign c1 = c1| plus:1 %}
                                  
                                  
                                  </div>
                                  </a>
                                  </div>       
                                  {% endcase %}
                                  
                                  {% endfor %}
                              {% if rpt1 == ture %}
                                      {% for media in product.media %}
                                      
                                      {% case media.media_type %}
                                      {% when 'image' %}
                                      <div class="text-center img_box" >
                                      <a href="javascript:void(0);" data-index="{{c1 }}" class="slide-link"  data-video="false" >
                                      <div class="img_wrap zoom">
                                      {%
                                      render 'rimg',
                                      class : 'img-fluid m-auto  lazyload',
                                      img: media.src,
                                      size: '700x700',
                                      lazy : false
                                      %}
                                      {% assign c1 = c1| plus:1 %}
                                      
                                      
                                      </div>
                                      </a>
                                      </div>       
                                      {% endcase %}
                                      
                                      {% endfor %}
                                {% endif %}
            {% endif %}
        </div>
        
      </div>
    </div>
    <div class="row">
     <div class="col-12 d-lg-block d-none pt-5 ps-5">
       {% if product.metafields.custom.gallery_videos.value != blank %}

         <div class="title-div"><h4>Moments</h4></div>
         {% endif %}
       <div class="video-desktop">
        {% render 'product-video-popup',product:product %}
        {% if product.metafields.global.product_video != blank %}
            {% if product.metafields.custom.gallery_videos.value == blank %}
        
          {% endif  %}
          {% if product.metafields.custom.gallery_videos.value == blank %}
          <div class="video_sec" style="width: 100%!important;">
             {{ product.metafields.global.product_video }}
          </div>
            {% else %}
            <div class="video_sec" >
             {{ product.metafields.global.product_video }}
          </div>
         {% endif %}
        {% endif %}
        </div>
        
      </div>
      <div class="col-12 d-lg-block d-none">
        <div class="reviews-desktop">
          {% render 'yopto-reviews' %}
        </div>
      </div>
    </div>

  </div>
  <div class="product__info-wrapper grid__item">
    <div id="ProductInfo-{{ section.id }}" class="product__info-container{% if section.settings.enable_sticky_info %} product__info-container--sticky{% endif %}">
      {%- assign product_form_id = 'product-form-' | append: section.id -%}
      <div >
            {%- form 'product', product, id: 'product-form-installment', class: ' caption-large' -%}
              <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
             
            {%- endform -%}
          </div>
      <div class="d-flex justify-content-between align-items-center">
        <div class="reviews-div">
          <div class="yotpo bottomLine"
               data-product-id="{{ product.id }}">
            {%- assign yotpo_offload_content = shop.metafields.yotpo.yotpo_offload_content %}
            {%- assign time_now = 'now' | date: '%s' %}
            {%- assign yotpo_live_time = shop.metafields.yotpo.yotpo_live | date: '%s' %}
            {%- assign diff_seconds_from_live = time_now | minus: yotpo_live_time %}
            {%- assign yotpo_bottomline_last_updated = product.metafields.yotpo.bottomline_update_time | date: '%s' %}
            {%- assign diff_seconds_from_last_bottomline_update = time_now | minus: yotpo_bottomline_last_updated %}
            {%- if yotpo_live_time and diff_seconds_from_live < 86400 or yotpo_bottomline_last_updated and diff_seconds_from_last_bottomline_update < 86400 -%}
            {%- assign yotpo_bottomline_obsolete = false %}
            {%- else %}
               {%- assign yotpo_bottomline_obsolete = true %}
            {%- endif %}
  
            {%- if yotpo_offload_content == 'yes' and yotpo_bottomline_obsolete != true -%}
            {{ product.metafields.yotpo.bottomline }}
            {%- endif %}
  
          </div>
        </div>
      
        
  
      
      
         {% for tag in product.tags %}
         {% if tag == "Offers_Final Sale" %}
         <div class="final_sale_wrapper"><p class="mb-0 final_sale"><strong>FINAL SALE ITEM </strong></p></div>
         {% endif %}
         {% endfor %}
        </div>
       <product-form class="product-form">
              <div class="product-form__error-message-wrapper" role="alert" hidden>
                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-error" viewBox="0 0 13 13">
                  <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
                  <circle cx="6.5" cy="6.5" r="5.5" fill="#EB001B" stroke="#EB001B" stroke-width="0.7"/>
                  <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
                  <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7" />
                </svg>
                <span class="product-form__error-message"></span>
              </div>

              {%- form 'product', product, id: product_form_id, class: 'form', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
             
      {%- for block in section.blocks -%}
        {%- case block.type -%}
        {%- when '@app' -%}
          {% render block %}
        {%- when 'text' -%}
          <p class="product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing{% elsif block.settings.text_style == 'subtitle' %} subtitle{% endif %}" {{ block.shopify_attributes }}>
            {{- block.settings.text -}}
          </p>
        {%- when 'title' -%}
          <h1 class="product__title" {{ block.shopify_attributes }}>
            {{ product.title | escape }}
            {% if product.vendor contains "Eyely" or product.vendor contains "happimess"  or product.vendor contains "ovalair"  %}<span class="gray-label">by eyely</span>{% endif %}
          </h1>
       
        {%- when 'price' -%}
          <div class="no-js-hidden d-flex flex-wrap align-items-center" id="price-{{ section.id }}" {{ block.shopify_attributes }}>
            {% assign selected_variant = product.selected_or_first_available_variant %}
            {% assign compare_at_price = selected_variant.compare_at_price %}
            {% assign price = selected_variant.price %}
            {% assign on_sale = false %}
            {% if compare_at_price > price %}
            {% assign on_sale = true %}
            {% endif %}
            {% assign product_available = product.available %}
            {%
              render 'product-badge',
            class_root: 'product',
            product_available: product_available,
            on_sale: on_sale,
            price: price,
            compare_at_price: compare_at_price,
            show_saved: true
            %}
              {%- render 'price', product: product, use_variant: true, show_badges: false, price_class: 'price--large' -%}
          </div>
          {% comment %}
          {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
            <div class="product__tax caption rte">
              {%- if shop.taxes_included -%}
                {{ 'products.product.include_taxes' | t }}
              {%- endif -%}
              {%- if shop.shipping_policy.body != blank -%}
                {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
              {%- endif -%}
            </div>
          {%- endif -%}
        {% endcomment %}
          
        {%- when 'description' -%}
          {% if product.metafields.global.Glance_Icons != blank %}
            <div class="icon_sec">
              <div class="icon-section">
                <p class="title-icon-sec">At a Glance</p>
                <div class="icon_inner">
                  {{ product.metafields.global.Glance_Icons }}
                </div>
              </div>
            </div>
            {% endif %}
          {%- if product.description != blank -%}
            <div class="product__description rte">
              <div class="accordion tab-detail mobile_accordian"><p><strong>PRODUCT DESCRIPTION</strong></p></div>
              <div class="panel">{% assign p1= product.description | split:"</p>" %}{{p1[0]}}</div>
              <div class="description">
              {{ product.description }}
              </div>
            </div>
          {%- endif -%}
          
        {%- when 'custom_liquid' -%}
          {{ block.settings.custom_liquid }}
        {%- when 'collapsible_tab' -%}
          <div class="product__accordion accordion" {{ block.shopify_attributes }}>
            <details>
              <summary>
                <div class="summary__title">
                  {% render 'icon-accordion', icon: block.settings.icon %}
                  <h2 class="h4 accordion__title">
                    {{ block.settings.heading | default: block.settings.page.title }}
                  </h2>
                </div>
                {% render 'icon-caret' %}
              </summary>
              <div class="accordion__content rte">
                {{ block.settings.content }}
                {{ block.settings.page.content }}
              </div>
            </details>
          </div>
        {%- when 'quantity_selector' -%}
          <div class="product-form__input product-form__quantity" {{ block.shopify_attributes }}>
            <label class="form__label d-none" for="Quantity-{{ section.id }}">
              {{ 'products.product.quantity.label' | t }}
            </label>

            <quantity-input class="quantity">
              <button class="quantity__button no-js-hidden" name="minus" type="button">
                <span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
                {% render 'icon-minus' %}
              </button>
              <input readonly class="quantity__input"
                  type="number"
                  name="quantity"
                  id="Quantity-{{ section.id }}"
                  min="1"
                  value="1"
                  form="product-form-{{ section.id }}"
                >
              <button class="quantity__button no-js-hidden" name="plus" type="button">
                <span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
                {% render 'icon-plus' %}
              </button>
            </quantity-input>
          </div>
        {%- when 'popup' -%}
            <modal-opener class="product-popup-modal__opener no-js-hidden" data-modal="#PopupModal-{{ block.id }}" {{ block.shopify_attributes }}>
              <button id="ProductPopup-{{ block.id }}" class="product-popup-modal__button link" type="button" aria-haspopup="dialog">{{ block.settings.text | default: block.settings.page.title }}</button>
            </modal-opener>
            <a href="{{ block.settings.page.url }}" class="product-popup-modal__button link no-js">{{ block.settings.text }}</a>
        {%- when 'share' -%}
          <share-button class="share-button" {{ block.shopify_attributes }}>
            <button class="share-button__button hidden">
              {% render 'icon-share' %}
              {{ block.settings.share_label | escape }}
            </button>
            <details>
              <summary class="share-button__button">
                {% render 'icon-share' %}
                {{ block.settings.share_label | escape }}
              </summary>
              <div id="Product-share-{{ section.id }}" class="share-button__fallback motion-reduce">
                <div class="field">
                  <span id="ShareMessage-{{ section.id }}" class="share-button__message hidden" role="status">
                  </span>
                  <input type="text"
                        class="field__input"
                        id="url"
                        value="{{ shop.url | append: product.url }}"
                        placeholder="{{ 'general.share.share_url' | t }}"
                        onclick="this.select();"
                        readonly
                  >
                  <label class="field__label" for="url">{{ 'general.share.share_url' | t }}</label>
                </div>
                <button class="share-button__close hidden no-js-hidden">
                  {% render 'icon-close' %}
                  <span class="visually-hidden">{{ 'general.share.close' | t }}</span>
                </button>
                <button class="share-button__copy no-js-hidden">
                  {% render 'icon-clipboard' %}
                  <span class="visually-hidden">{{ 'general.share.copy_to_clipboard' | t }}</span>
                </button>
              </div>
            </details>
          </share-button>
          <script src="{{ 'share.js' | asset_url }}" defer="defer"></script>
        {%- when 'variant_picker' -%}
         {% assign quote1= false %}
          {%- unless product.has_only_default_variant -%}
            {%- if block.settings.picker_type == 'button' -%}
              <variant-radios class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}" {{ block.shopify_attributes }}>
                  {% assign optionSize = product.options_with_values | size %}
                {%- for option in product.options_with_values -%}
                                     {% assign in = forloop.index %}
                  {% assign option_name = option.name | downcase %}
                 {% assign option_name_1 = option_name %}
                    {% if option_name == 'color' %}
                        {% assign swatch_option_key = 'option' | append: forloop.index %}
                      <fieldset class="js product-form__input color-options">
                        <legend class="form__label">{{ option.name }} :<span class="option-val op-val-{{in}}">{%- for value in option.values -%}  {% if option.selected_value == value %}{{value}}{% endif %}{% endfor %}</span></legend>
                       {% assign values = "" %}
                       
                        {%- for value in option.values -%}
                           {% assign fl = true %}
                           {% assign availableVar = true %}

                                {% unless values contains value %}
                                {% assign values = values | join: ';' %}
                                {% assign values = values | append: ';' | append: value %}
                                {% assign values = values | split: ';' %}     
                                {% endunless %}  
                            {% for variant in product.variants %}
                            {% assign option_value_downcased = variant[swatch_option_key] | downcase %}
                            {% assign d_value = value | downcase %}
                              {% if option_value_downcased == d_value and fl == true %}
                                  {% if variant.image != blank %}
                                      {% assign v_img = variant.image %}
                                  {% else%}
                                      {% assign v_img = product.featured_image %}
                                      {% endif%}		
                        {% unless variant.available %}
                                      {% assign availableVar = false %}
                                  {% endunless %}
                        
                                  {% assign fl = false %}
                              {% endif %}
                            {% endfor %}     
                        <div class="rd-wrapper">
                          <input type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index}}"                                  name="{{ option.name }}"
                                value="{{ value | escape }}"
                                 data-option-id="{{in}}" data-v="{{value | replace:'"','' }}"
                                form="product-form-{{ section.id }}"  class="options-vals option-value-input"
                                {% if option.selected_value == value %}checked{% endif %}
                          >
                          <label  class="color-label" data-bs-container="body" data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{ value }}" for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}">
                                     <img class="image-content__image"
                       loading="lazy"
                       src="{{ v_img | img_url: 'x64' }}">      
                          </label>
                        </div>
                      
                   
                        {%- endfor -%}
                       {% if product.metafields.custom.other_products.value != blank %}
                                {% for prd in product.metafields.custom.other_products.value %}
                                  {% for opt in prd.options_with_values %}
                                  {% assign option_name = opt.name | downcase %}
                                    {% if  option_name == option_name_1 %}
                                      {% assign swatch_option_key =  'option' | append: forloop.index %}
                                      {% for value in opt.values %}   
                        
                                          
                                          {% assign show_swatch = true %}
                                          {% for v in values %}
                                              {% if v == value %}
                                                  {% assign show_swatch = false %}
                                              {% endif %}
                                          {% endfor %}
                                          {% if show_swatch %}
                                              {% assign values = values | join: ';' %}
                                              {% assign values = values | append: ';' | append: value %}
                                              {% assign values = values | split: ';' %}     
                                              {% assign v_add = true %}
                                                  {% assign v_img1 = '' %}
                                              {% for variant in prd.variants %}
                                                       {% assign option_value_downcased1 = variant[swatch_option_key] | downcase %}
                                                        {% assign d_value1 = value | downcase %}
                      
                                                        {% if option_value_downcased1 == d_value1 and v_add == true %}
                                                              {% assign v_url = variant.url %}
                                                              {% if variant.image != blank %}
                                                             
                                                              {% assign v_img1 = variant.image %}
                                                              {% assign v_add = false %}
                                                              {% else%}
                                                            
                                                              {% assign v_img1 = prd.featured_image %}
                                                              {% endif %}		
                                                        {% endif %}
                                                   
                                               {% endfor %}
                                                       {% if v_add == true %}
                                                              {% assign v_img1 = prd.featured_image %}
                                                          {% assign v_add = false %}  
                                                      {% endif %}
                                          
                                               {% if v_img1 != blank and v_add  == false %} 
                                                  
                                               <div class="rd-wrapper" >
                                                  <a href="{{v_url}}">
                                                    <label  class="color-label" data-bs-container="body" data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{ value }}" for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}">
                                                      <img class="image-content__image"
                                                           loading="lazy"
                                                           src="{{ v_img1 | img_url: 'x64' }}">      
                                                    </label>
                                                  </a>
                                                </div>
                                          {% endif %}
                                              
                                          {% endif %}
                                      {% endfor %}
                              {% endif %}
                          {% endfor %}
                      {% endfor %}
              
                        {% endif %}
  
                      
                      </fieldset>
                      
